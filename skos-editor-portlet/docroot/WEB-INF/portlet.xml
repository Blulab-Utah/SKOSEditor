<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>skos-editor</portlet-name>
		<display-name>SKOS Editor</display-name>
		<portlet-class>edu.ucsd.nlp.portlet.SKOSEditorPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/jsp/skos.jsp</value>
		</init-param>
		<init-param>
			<name>error-jsp</name>
			<value>/jsp/error.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>SKOS Editor</title>
			<short-title>SKOS Editor</short-title>
			<keywords>SKOS Editor</keywords>
		</portlet-info>
	</portlet>
</portlet-app>